generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Edge {
  id         Int @id @default(autoincrement())
  fromNodeId Int
  toNodeId   Int
}

model Node {
  id   Int    @id @default(autoincrement())
  name String @unique
}
